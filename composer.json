{
  "name": "customd/laravel-addressable",
  "description": "Laravel Addressable is a Laravel package to manage addresses that belong to your models. You can add addresses to any eloquent model with ease.",
  "type": "library",
  "keywords": [
    "city",
    "state",
    "model",
    "postal",
    "street",
    "address",
    "laravel",
    "country",
    "primary",
    "billing",
    "shipping",
    "eloquent",
    "latitude",
    "longitude",
    "addressable",
    "polymorphic"
  ],
  "license": "MIT",
  "homepage": "https://customd.com",
  "support": {
    "email": "help@customd.com",
    "issues": "https://github.com/customd/laravel-addresses/issues",
    "source": "https://github.com/customd/laravel-addresses",
    "docs": "https://github.com/customd/laravel-addresses/blob/master/README.md"
  },
  "authors": [
    {
      "name": "Custom D Ltd",
      "homepage": "https://customd.com",
      "email": "help@customd.com"
    }
  ],
  "require": {
    "php": "^8.0.0",
    "illuminate/console": "^9.0|^10.0|^11.0",
    "illuminate/database": "^9.0|^10.0|^11.0",
    "illuminate/support": "^9.0|^10.0|^11.0",
    "jackpopp/geodistance": "^1.2.0",
    "rinvex/countries": "^8.0.0",
    "spatie/laravel-package-tools": "^1.9.2"
  },
  "require-dev": {
    "illuminate/container": "^9.0|^10.0|^11.0",
    "nunomaduro/collision": "^6.0|^7.0",
    "nunomaduro/larastan": "^2.0.1",
    "orchestra/testbench": "^7.0|^8.0|^9.0",
    "phpstan/extension-installer": "*",
    "phpstan/phpstan-deprecation-rules": "*",
    "phpstan/phpstan-phpunit": "*",
    "phpunit/phpunit": "^9.5.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "CustomD\\Addressable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CustomD\\Addressable\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CustomD\\Addressable\\AddressesServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
